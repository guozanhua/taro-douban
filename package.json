{
  "name": "taro-douban",
  "version": "1.0.0",
  "private": true,
  "description": "基于 taro 实现豆瓣电影微信小程序 @zce",
  "main": "index.js",
  "scripts": {
    "build:weapp": "taro build --type weapp",
    "build:h5": "taro build --type h5",
    "build:rn": "taro build --type rn",
    "dev:weapp": "npm run build:weapp -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev:proxy1": "cross-env METHOD=proxy1 npm-run-all --parallel dev:weapp proxy1",
    "dev:proxy2": "cross-env METHOD=proxy2 npm-run-all --parallel dev:weapp proxy1",
    "dev:nginx": "cross-env METHOD=nginx npm run dev:weapp",
    "proxy1": "node douban/app.js",
    "proxy2": "node douban/proxy.js"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@tarojs/async-await": "^1.0.7",
    "@tarojs/components": "^1.0.1",
    "@tarojs/redux": "^1.0.7",
    "@tarojs/redux-h5": "^1.0.1",
    "@tarojs/router": "^1.0.1",
    "@tarojs/taro": "^1.0.1",
    "@tarojs/taro-h5": "^1.0.1",
    "@tarojs/taro-weapp": "^1.0.7",
    "co-request": "^1.0.0",
    "cross-env": "^5.2.0",
    "flyio": "^0.6.2",
    "http-proxy-middleware": "^0.19.0",
    "koa": "^2.5.3",
    "koa-router": "^7.4.0",
    "koa2-connect": "^1.0.2",
    "nervjs": "^1.3.0",
    "npm-run-all": "^4.1.3",
    "promise-polyfill": "^8.1.0",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "wx": "^0.0.36"
  },
  "devDependencies": {
    "@tarojs/plugin-babel": "^1.0.7",
    "@tarojs/plugin-csso": "^1.0.1",
    "@tarojs/plugin-sass": "^1.0.1",
    "@tarojs/plugin-uglifyjs": "^1.0.1",
    "@tarojs/webpack-runner": "^1.0.1",
    "@types/react": "^16.4.8",
    "@types/webpack-env": "^1.13.6",
    "babel-eslint": "^8.2.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-jsx-stylesheet": "^0.6.5",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "compass-mixins": "^0.12.10",
    "eslint": "^4.19.1",
    "eslint-config-taro": "^1.0.1",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-react": "^7.8.2",
    "eslint-plugin-taro": "^1.0.1",
    "sass-bem": "^2.6.5"
  }
}
